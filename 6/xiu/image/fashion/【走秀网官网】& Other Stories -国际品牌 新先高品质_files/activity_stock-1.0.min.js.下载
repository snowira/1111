/*! power by xiu front end team  build by grunt  2014-08-25 */
function readStock(a,b,c){var d=$("input[name=actids]");if(!(d.length<1)){var e=d.eq(b).val(),f="",g=$("input[name=goodsSNs]"),f=g.length>0?g.eq(0).val():"",h=f.split(","),i=Math.ceil(h.length/60),j=0;"undefined"!=typeof c&&c>0&&(j=c);var k=h.slice(60*j,60*(j+1)).join(","),l=url.wcsurl+"SecondInventoryCmd?"+url.parms+"&activityId="+e+"&goodsSns="+k+"&timeStamp="+new Date+"&jsoncallback=?";$.ajax({type:"POST",url:l,dataType:"json",success:function(b){b=jsonData(b),list=jsonGet(b.stock),$.each(list,function(b,c){var d=e+"_"+c.goodsId,f=$("#stock_"+d),g=c.inventory;if(0>g&&(g=0),0==c.onSale&&(g=0),f.text(g),5>g?f.parent().show():f.parent().hide(),0==g){var h=$("#flag_stock_"+d);null!=h&&$("#flag_stock_"+d).append(a)}})},error:function(){},complete:function(){j++,i>j&&readStock(a,b,j)}})}}