/*! power by xiu front end team  build by grunt  2014-08-25 */
function getPartnumberSize(index,indexPage){var SNs="",goodsSN1s=$("input[name=goodsIds]");null!=goodsSN1s&&goodsSN1s.length>0&&(SNs=goodsSN1s.eq(0).val());var goodsSns=SNs.split(","),page=Math.ceil(goodsSns.length/60),currentPage=0;"undefined"!=typeof indexPage&&indexPage>0&&(currentPage=indexPage);var pageSns=goodsSns.slice(60*currentPage,60*(currentPage+1)).join(","),ids=pageSns.split(",");$.ajax({type:"GET",url:"http://search.xiu.com/skulist.html",data:{ids:pageSns},dataType:"jsonp",jsonp:"callback",contentType:"application/x-www-form-urlencoded; charset=utf-8",success:function(data){var datajson=eval(data);if(datajson=datajson[0],null!=datajson&&""!=datajson)for(var i=0;i<ids.length;i++){var subdata=datajson[ids[i]],htmlConents="";if(void 0!=subdata&&null!=subdata&&subdata.length>0){for(var j=0;j<subdata.length;j++)htmlConents+=0==subdata[j].c?'<li class="ssl_item box"><span class="line-through">'+subdata[j].s+"</span></li>":'<li class="ssl_item"><span>'+subdata[j].s+"</span></li>";$("#J_sizeWrap_"+ids[i]+" ul").html(htmlConents)}}},error:function(){},complete:function(){currentPage++,page>currentPage&&getPartnumberSize(index,currentPage)}})}$(document).ready(function(){getPartnumberSize(0)});